<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/artist_layout"
        tools:context="com.premiosnucleourbano.nucleourbano.ArtistActivity">

    <fragment android:name="com.premiosnucleourbano.nucleourbano.fragment.ArtistaFragment"
        android:id="@+id/artist_frag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/all_artist" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_three_id">
    </com.google.android.gms.ads.AdView>

    </RelativeLayout>
